<div class="index">

  <h1>Slack SMS</h1>

  <% if logged_in? %>
    <%= current_user.id %>
    <%= current_user.slack_access_token %>
    <%= current_user.phone_number %>
  <% end %>

  <h2>Interact with your team on Slack using SMS</h2>

  <a href="/sign-into-slack" class="button">Sign in with Slack</a>

</div>
